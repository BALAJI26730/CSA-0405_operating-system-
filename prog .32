#include <stdio.h>

int main() {
    int frames, pages, i, j, k, pageFaults = 0, least, count = 0;
    int frame[10], ref[30], lastUsed[10];

    printf("Enter number of frames: ");
    scanf("%d", &frames);

    printf("Enter number of pages: ");
    scanf("%d", &pages);

    printf("Enter page reference string:\n");
    for (i = 0; i < pages; i++)
        scanf("%d", &ref[i]);

    for (i = 0; i < frames; i++) {
        frame[i] = -1;
        lastUsed[i] = 0;
    }

    for (i = 0; i < pages; i++) {
        int found = 0;
        for (j = 0; j < frames; j++) {
            if (frame[j] == ref[i]) {
                found = 1;
                lastUsed[j] = i;
                break;
            }
        }

        if (!found) {
            if (count < frames) {
                frame[count] = ref[i];
                lastUsed[count] = i;
                count++;
            } else {
                least = 0;
                for (j = 1; j < frames; j++)
                    if (lastUsed[j] < lastUsed[least])
                        least = j;
                frame[least] = ref[i];
                lastUsed[least] = i;
            }
            pageFaults++;
        }

        printf("Frame: ");
        for (k = 0; k < frames; k++)
            if (frame[k] != -1) printf("%d ", frame[k]);
        printf("\n");
    }

    printf("Total Page Faults = %d\n", pageFaults);
    return 0;
}
